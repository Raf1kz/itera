{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "smoke": "playwright test",
    "deadcode": "knip --reporter json",
    "depcheck": "depcheck --skip-missing=true --json",
    "qa": "npm run typecheck && npm run lint && npm run test && npm run build",
    "qa:score": "node tools/qa-score.mjs",
    "deploy:functions": "supabase functions deploy generate-flashcards --project-ref $PROJECT_REF",
    "logs:functions": "supabase functions logs generate-flashcards --project-ref $PROJECT_REF",
    "qa:fix": "npm run format:fix && npm run lint:fix && npm run typecheck && npm run test",
    "analyze": "vite build --mode analyze",
    "audit": "npm audit --production",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.53.4",
